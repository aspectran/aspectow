<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users">

    <insert id="insertGuest" parameterType="club.textchat.user.UserInfo" useGeneratedKeys="true" keyProperty="userNo">
        insert into guests (user_nm, ip_addr) values (#{username}, #{ipAddr})
    </insert>

</mapper>